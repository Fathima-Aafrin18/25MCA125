package Arrays;

class SparseMatrix {
    public static void main(String[] args) {
        int[][] A = {{0,0,3},{0,0,0},{1,0,0}};
        int zero=0, total=9;

        for(int i=0;i<3;i++){
            for(int j=0;j<3;j++){
                if(A[i][j]==0) zero++;
            }
        }

        System.out.println(zero > total/2 ? "Sparse Matrix" : "Not Sparse Matrix");
    }
}
