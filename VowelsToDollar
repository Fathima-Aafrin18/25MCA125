package String;

class VowelsToDollar {
    public static void main(String[] args) {
        String s = "Hello";
        String result = "";

        for (char c : s.toCharArray()) {
            if ("aeiouAEIOU".indexOf(c) != -1)
                result += "$";
            else
                result += c;
        }
        System.out.println(result);
    }
}
