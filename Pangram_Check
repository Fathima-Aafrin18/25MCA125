package String;

class PangramCheck {
    public static void main(String[] args) {
        String s = "The quick brown fox jumps over a lazy dog".toLowerCase();
        boolean isPangram = true;

        for (char c = 'a'; c <= 'z'; c++) {
            if (!s.contains(c + "")) {
                isPangram = false;
                break;
            }
        }
        System.out.println(isPangram ? "Pangram" : "Not Pangram");
    }
}
