package Arrays;

class EqualMatrices {
    public static void main(String[] args) {
        int[][] A = {{1,2},{3,4}};
        int[][] B = {{1,2},{3,4}};
        boolean equal = true;

        for(int i=0;i<2;i++){
            for(int j=0;j<2;j++){
                if(A[i][j] != B[i][j]){
                    equal = false;
                    break;
                }
            }
        }

        System.out.println(equal ? "Matrices are Equal" : "Matrices are Not Equal");
    }
}
