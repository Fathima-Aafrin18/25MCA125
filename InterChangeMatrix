package Arrays;

class InterchangeMatrix {
    public static void main(String[] args) {
        int[][] A = {{5,6,7},{8,9,10}};

        for(int i=0;i<3;i++){
            for(int j=0;j<2;j++){
                System.out.print(A[j][i]+" ");
            }
            System.out.println();
        }
    }
}
