package String;

class SearchCharacter {
    public static void main(String[] args) {
        String s = "Hello";
        char ch = 'e';
        boolean found = false;

        for (int i = 0; i < s.length(); i++) {
            if (s.charAt(i) == ch) {
                found = true;
                break;
            }
        }
        System.out.println(found ? "Found" : "Not Found");
    }
}
